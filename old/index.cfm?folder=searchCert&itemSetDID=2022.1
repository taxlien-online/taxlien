function validateLoginName (inputObj) {
// Validates the login name (username)
if (inputObj == '') { 
alert('Username can not be empty');
return false;
} else { 
return true;
}
}
function validatePassword(inputObj,displayAlert) {
// Validates the password. displayAlert = false allows validation without the alert message.
if ( inputObj == '') { 
if (displayAlert) alert('Password can not be empty');
return false;
} else { 
return true;
}
}
function submitLoginAction(theAction) {
// Submit a login action 
// If the user press cancel, we don't want to keep returning to login.
if (theAction == 'cancel') document.loginFrm.action = 'index.cfm?folder=home&loginAction=nothing'; // In this case loginAction must be in the URL. For some unknown reason document.loginFrm.loginAction.value = theAction have not effect here.
if (theAction == 'nothing') document.loginFrm.action = 'index.cfm?folder=home';
if (theAction == 'logout' || (theAction == 'agreement' && document.loginFrm.response.value == 'NO') ) {
document.loginFrm.action = 'index.cfm?folder=home&loginAction=logout'; 
// In this case loginAction must be in the URL. For some unknown reason document.loginFrm.loginAction.value = theAction have not effect here.
}
//validate the login form before the submit. Password may not exists when the form is requesting just the loginName before presenting the security question for a password reset.
if ((theAction == "agreement") || validateLoginName(document.loginFrm.loginName) 
&& 
(document.loginFrm.password == null || validatePassword(document.loginFrm.password, true))) {
document.loginFrm.loginAction.value = theAction;
document.loginFrm.submit();
}else{
return false;
}
}
function submitLoginActionForm(theAction) {
